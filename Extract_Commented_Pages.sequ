		/HandlerName [/a /JavaScript]
		/Params [/c <<			/ScriptCode [/t (//////////////////////////////////////////////\r\n///////////////////////////////////////////////\r\n//\r\n//   Extract Comment Pages Action Script\r\n//\r\n//   by Thom Parker, WindJack Solutions, Inc.\r\n//      www.windjack.com, www.pdfscripting.com\r\n//   for Adobe Systems Inc. www.adobe.com\r\n//\r\n//       NOTE: Only for use in an Action Script\r\n//       Requires:  Acrobat 10 or later\r\n//\r\n//   Extracts all pages from the current PDF that contain markup\r\n//   annotations.  File is either added to a combined extracted pages file\r\n//   or renamed with "_ExtractedCommments" appended\r\n//   to the end of the file and saved to either the original folder\r\n//   or a folder indicated by the user.  Also creates a summary of the \r\n//   pages extracted\r\n//\r\n/////\r\n//\r\n//   Version 1.1 - 11/5/2010\r\n\r\nvar bRedo = true;\r\n\r\ntry{global.oExtrSumDoc.path;}\r\ncatch\(e\)\r\n{// Perform startup action if summary document is invalid\r\n\r\n  var CreateExtSumFields = app.trustedFunction\(function\(oDoc, cName, bAppendExt\)\r\n  {\r\n     oDoc.bookmarkRoot.createChild\("Summary Page","this.pageNum=0"\);\r\n     var oFld = oDoc.addField\("Lab1","text",0,[54,423,600,391]\);\r\n     oFld.textSize = 22;\r\n     oFld.textFont = "Helvetica-Bold";\r\n     oFld.textColor = ["G",0];\r\n     oFld.strokeColor = ["T"];\r\n     oFld.borderStyle = "solid";\r\n     oFld.fillColor =  ["T"];\r\n     oFld.value = "Summary of Extracted Comment Pages";\r\n      \r\n     oFld = oDoc.addField\("Lab2a","text",0, [54,396,600,376]\);\r\n     oFld.textSize = 12;\r\n     oFld.textFont = "Helvetica-Oblique";\r\n     oFld.textColor = ["G",0];\r\n     oFld.strokeColor = ["T"];\r\n     oFld.borderStyle = "solid";\r\n     oFld.fillColor =  ["T"];\r\n     oFld.value = "\(Subsequent Extract Comment Actions will add to this file, it must be closed to start a new Summary\)";\r\n\r\n     var nAddH = 0;\r\n     if\(bAppendExt\)\r\n     {\r\n        nAddH = 20;\r\n        oFld = oDoc.addField\("Lab2c","text",0, [54,376,600,356]\);\r\n        oFld.textSize = 12;\r\n        oFld.textFont = "Helvetica-Oblique";\r\n        oFld.textColor = ["G",0];\r\n        oFld.strokeColor = ["T"];\r\n        oFld.borderStyle = "solid";\r\n        oFld.fillColor =  ["T"];\r\n        oFld.value = "\(Extracted Pages have been appended to the file, listed in bookmarks\)";\r\n     }\r\n\r\n     oDoc.addAnnot\({type:"Line", page:0, points:[[36,373-nAddH],[613,373-nAddH]], width:2, style:"S",\r\n                    strokeColor:color.black, arrowEnd:"None",arrowBegin:"None"}\);\r\n\r\n     oFld = oDoc.addField\("Lab2","text",0, [60,373-nAddH,384,323-nAddH]\);\r\n     oFld.multiline = true;\r\n     oFld.textSize = 12;\r\n     oFld.textFont = "Helvetica";\r\n     oFld.textColor = ["G",0];\r\n     oFld.strokeColor = ["T"];\r\n     oFld.borderStyle = "solid";\r\n     oFld.fillColor =  ["T"];\r\n     oFld.value = "Name: " + cName + "\\nDate: " + util.printd\("ddd mmm dd, yyyy",new Date\(\)\)\r\n                  + "\\nTime: " + util.printd\("hh:mm tt",new Date\(\)\);\r\n\r\n     app.beginPriv\(\);\r\n     oDoc.flattenPages\(\);\r\n     app.endPriv\(\);\r\n\r\n     var oFld = oDoc.addField\("NumFiles","text",0,[60,316-nAddH,300,296-nAddH]\);\r\n     oFld.readonly = true;\r\n     oFld.textSize = 14;\r\n     oFld.textFont = "Helvetica-Bold";\r\n     oFld.textColor = ["G",0];\r\n     oFld.strokeColor = ["T"];\r\n     oFld.borderStyle = "solid";\r\n     oFld.fillColor =  ["T"];\r\n     oFld.value = "Files Processed: 0";\r\n\r\n     if\(bAppendExt\)\r\n     {\r\n        var oFld = oDoc.addField\("ShowAtt","button",0,[460,318-nAddH ,600 ,294-nAddH]\);\r\n        oFld.textSize = 12;\r\n        oFld.textFont = "Helvetica-Bold";\r\n        oFld.textColor = ["G",0];\r\n        oFld.strokeColor = ["G",0];\r\n        oFld.borderStyle = "solid";\r\n        oFld.fillColor =  ["G",2/3];\r\n        oFld.buttonSetCaption\("Show Extracted Pages"\);\r\n        oFld.setAction\("MouseUp","if\(eval\(this.viewState.toSource\(\)\).overViewMode != 3\){"+\r\n                                    "this.viewState = {overViewMode:3};event.target.buttonSetCaption\('Hide Bookmarks'\);}" +\r\n                                  "else{this.viewState = {overViewMode:1};event.target.buttonSetCaption\('Show Extracted Pages'\);}"\);\r\n     }\r\n\r\n     var oFld = oDoc.addField\("FilesProcessed","listbox",0,[60,290-nAddH,600,36]\);\r\n     oFld.readonly = false;\r\n     oFld.commitOnSelChange = true;\r\n     oFld.textSize = 12;\r\n     oFld.textFont = "Helvetica";\r\n     oFld.textColor = ["G",0];\r\n     oFld.strokeColor = ["G",0];\r\n     oFld.borderStyle = "solid";\r\n     oFld.fillColor =  ["T"];\r\n     oFld.setAction\("Format","this.dirty = false"\);\r\n\r\n  }\);\r\n\r\n//Acrobat JavaScript Dialog\r\n//Created by DialogDesigner from WindJack Solutions\r\nglobal.ExtrCmmtPgsDlg =\r\n{\r\n\r\n    result:"cancel",\r\n    DoDialog: function\(\){return app.execDialog\(this\);},\r\n    bAppendToSum:false,\r\n    bSaveExtract:false,\r\n    strSaveLoc:"Same",\r\n    strSavePath:"",\r\n    strSummaryPath:"",\r\n    strSummaryName:"Comment Summary",\r\n    GetRadioSel:function\(oRslts,aCtrls\){\r\n      for\(var strRtn=aCtrls[0];aCtrls.length>0;strRtn=aCtrls.pop\(\)\){\r\n        if\(oRslts[strRtn] == true\)\r\n          return strRtn;\r\n      }\r\n      return "";\r\n    },\r\n    initialize: function\(dialog\)\r\n    {\r\n        var dlgInit = \r\n        {\r\n            "stat": "Pages containing comments\(or markup annotations\), for PDF files processed will be extracted.  A summary of all files processed is created.",\r\n            "SmNm": this.strSummaryName,\r\n                "Apnd": this.bAppendToSum,\r\n                "SvEx": this.bSaveExtract,\r\n                "FPth": this.strSavePath,\r\n                "SPth": this.strSummaryPath,\r\n        };\r\n        dlgInit[this.strSaveLoc] = true;\r\n        dialog.load\(dlgInit\);\r\n        var bFldr = \(this.strSaveLoc == "Fldr"\);\r\n        dialog.enable\({"Same":this.bSaveExtract,"Fldr":this.bSaveExtract, "Brse":this.bSaveExtract && bFldr, "FPth":this.bSaveExtract && bFldr}\);\r\n    },\r\n    validate: function\(dialog\)\r\n    {\r\n        var oRslt = dialog.store\(\);\r\n        if\(!\(oRslt["Apnd"] || oRslt["SvEx"]\)\)\r\n        {  \r\n             app.alert\("At least one of the options \\"Append Extracted Pages\\" or \\"Save Extracted Pages\\" must be selected"\);\r\n            return false;\r\n        }\r\n        \r\n        var rgEmpty = /^\\s*$/;\r\n        if\(oRslt["SvEx"] && oRslt["Fldr"] && rgEmpty.test\(oRslt["FPth"]\)\)\r\n        {  \r\n             app.alert\("File Folder for Extracted Pages Must be Specified"\);\r\n             return false;\r\n        }\r\n        \r\n        if\(rgEmpty.test\(oRslt["SPth"]\)\)\r\n        {  \r\n             app.alert\("Summary Location Must be Specified"\);\r\n             return false;\r\n        }\r\n        \r\n        if\(rgEmpty.test\(oRslt["SmNm"]\)\)\r\n        {  \r\n             app.alert\("Summary Name Must be Specified"\);\r\n             return false;\r\n        }\r\n        return true;\r\n    },\r\n    commit: function\(dialog\)\r\n    {\r\n        var oRslt = dialog.store\(\);\r\n        this.bAppendToSum = oRslt["Apnd"];\r\n        this.bSaveExtract = oRslt["SvEx"];\r\n        this.strSaveLoc = this.GetRadioSel\(oRslt,["Same","Fldr"]\);\r\n        this.strSavePath = oRslt["FPth"];\r\n        this.strSummaryPath = oRslt["SPth"];\r\n        this.strSummaryName = oRslt["SmNm"];\r\n    },\r\n    "But1": app.trustedFunction\(function\(dialog\)\r\n    {\r\n        try{\r\n          app.beginPriv\(\);\r\n          var cRslt = Collab.browseForFolder\(\);\r\n          app.endPriv\(\);\r\n          if\(\(typeof\(cRslt\)!="undefined"\) && \(cRslt != null\)\)\r\n             dialog.load\({"SPth":cRslt}\);\r\n        }catch\(e\){\r\n          app.alert\("Cannot Browse For Folder"\);\r\n        }\r\n    }\),\r\n    "Brse": app.trustedFunction\(function\(dialog\)\r\n    {\r\n        try{\r\n          app.beginPriv\(\);\r\n          var cRslt = Collab.browseForFolder\(\);\r\n          app.endPriv\(\);\r\n          if\(\(typeof\(cRslt\)!="undefined"\) && \(cRslt != null\)\)\r\n             dialog.load\({"FPth":cRslt}\);\r\n        }catch\(e\){\r\n          app.alert\("Cannot Browse For Folder"\);\r\n        }\r\n    }\),\r\n    "Fldr": function\(dialog\)\r\n    {\r\n        var oRslt = dialog.store\(\);\r\n        var bFldr = oRslt["Fldr"];\r\n        dialog.enable\({"Brse":bFldr, "FPth":bFldr}\);\r\n    },\r\n    "Same": function\(dialog\)\r\n    {\r\n        var oRslt = dialog.store\(\);\r\n        var bFldr = oRslt["Fldr"];\r\n        dialog.enable\({"Brse":bFldr, "FPth":bFldr}\);\r\n    },\r\n    "SvEx": function\(dialog\)\r\n    {\r\n        var oRslt = dialog.store\(\);\r\n        var bSvOn = oRslt["SvEx"];\r\n        var bFldr = oRslt["Fldr"];\r\n        dialog.enable\({"Same":bSvOn,"Fldr":bSvOn, "Brse":bSvOn && bFldr, "FPth":bSvOn && bFldr}\);\r\n    },\r\n    description:\r\n    {\r\n        name: "Extract Comment Pages Action",\r\n        elements:\r\n        [\r\n            {\r\n                type: "view",\r\n                elements:\r\n                [\r\n                    {\r\n                        type: "view",\r\n                        char_height: 10,\r\n                        elements:\r\n                        [\r\n                            {\r\n                                type: "static_text",\r\n                                item_id: "stat",\r\n                                name: "Put Dialog Controls Here",\r\n                                width: 391,\r\n                                height: 32,\r\n                                char_width: 15,\r\n                                alignment: "align_fill",\r\n                                font: "dialog",\r\n                            },\r\n                            {\r\n                                type: "check_box",\r\n                                item_id: "Apnd",\r\n                                name: "Append Extracted Pages to Summary File",\r\n                                variable_Name: "bAppendToSum",\r\n                            },\r\n                            {\r\n                                type: "check_box",\r\n                                item_id: "SvEx",\r\n                                name: "Save Extracted Pages to File",\r\n                                variable_Name: "bSaveExtract",\r\n                            },\r\n                            {\r\n                                type: "view",\r\n                                align_children: "align_row",\r\n                                elements:\r\n                                [\r\n                                    {\r\n                                        type: "gap",\r\n                                        item_id: "gap1",\r\n                                        width: 25,\r\n                                    },\r\n                                    {\r\n                                        type: "radio",\r\n                                        item_id: "Same",\r\n                                        group_id: "SvLc",\r\n                                        name: "Same Location as Original PDF",\r\n                                        variable_Name: "strSaveLoc",\r\n                                    },\r\n                                ]\r\n                            },\r\n                            {\r\n                                type: "view",\r\n                                align_children: "align_row",\r\n                                elements:\r\n                                [\r\n                                    {\r\n                                        type: "gap",\r\n                                        item_id: "gap2",\r\n                                        width: 25,\r\n                                    },\r\n                                    {\r\n                                        type: "radio",\r\n                                        item_id: "Fldr",\r\n                                        group_id: "SvLc",\r\n                                        name: "In Folder",\r\n                                    },\r\n                                    {\r\n                                        type: "button",\r\n                                        item_id: "Brse",\r\n                                        name: "Browse for Folder",\r\n                                    },\r\n                                ]\r\n                            },\r\n                            {\r\n                                type: "view",\r\n                                align_children: "align_row",\r\n                                alignment: "align_fill",\r\n                                elements:\r\n                                [\r\n                                    {\r\n                                        type: "gap",\r\n                                        item_id: "gap2",\r\n                                        width: 25,\r\n                                    },\r\n                                    {\r\n                                        type: "edit_text",\r\n                                        item_id: "FPth",\r\n                                        variable_Name: "strSavePath",\r\n                                        alignment: "align_fill",\r\n                                    },\r\n                                ]\r\n                            },\r\n                            {\r\n                                type: "view",\r\n                                align_children: "align_row",\r\n                                alignment: "align_fill",\r\n                                elements:\r\n                                [\r\n                                    {\r\n                                        type: "static_text",\r\n                                        item_id: "sta0",\r\n                                        name: "Summary Location:",\r\n                                        alignment: "align_right",\r\n                                        font: "dialog",\r\n                                    },\r\n                                    {\r\n                                        type: "button",\r\n                                        item_id: "But1",\r\n                                        name: "Browse for Folder",\r\n                                    },\r\n                                ]\r\n                            },\r\n                            {\r\n                                type: "edit_text",\r\n                                item_id: "SPth",\r\n                                variable_Name: "strSummaryPath",\r\n                                alignment: "align_fill",\r\n                            },\r\n                            {\r\n                                type: "view",\r\n                                align_children: "align_top",\r\n                                alignment: "align_fill",\r\n                                elements:\r\n                                [\r\n                                    {\r\n                                        type: "static_text",\r\n                                        item_id: "sta1",\r\n                                        name: "Summary Name:",\r\n                                        alignment: "align_right",\r\n                                        font: "dialog",\r\n                                    },\r\n                                    {\r\n                                        type: "edit_text",\r\n                                        item_id: "SmNm",\r\n                                        variable_Name: "strSummaryName",\r\n                                        width: 174,\r\n                                        height: 23,\r\n                                        char_width: 8,\r\n                                        alignment: "align_fill",\r\n                                    },\r\n                                ]\r\n                            },\r\n                        ]\r\n                    },\r\n                    {\r\n                        type: "view",\r\n                        align_children: "align_row",\r\n                        alignment: "align_fill",\r\n                        elements:\r\n                        [\r\n                    {\r\n                        type: "ok_cancel",\r\n                        cancel_name: "Cancel Action",\r\n                    },\r\n                            {\r\n                                type: "gap",\r\n                                item_id: "gap1",\r\n                                width: 40,\r\n                                height: 5,\r\n                            },\r\n                            {\r\n                                type: "static_text",\r\n                                item_id: "sta1",\r\n                                name: "version 1.1  11/5/2010",\r\n                                alignment: "align_right",\r\n                            },\r\n                        ]\r\n                    },\r\n\r\n                ]\r\n            },\r\n        ]\r\n    }\r\n};\r\n\r\n\r\n  // Setup initial document parameters\r\n  global.ExtrCmmtPgsDlg.strSavePath = event.target.path.replace\(/\\/[^\\/]+$/,"/"\);\r\n  global.ExtrCmmtPgsDlg.strSummaryPath = global.ExtrCmmtPgsDlg.strSavePath;\r\n  global.ExtrCmmtPgsDlg.strSummaryName = "Extract Comment Summary";\r\n  global.bSkipSumSave = false;\r\n\r\n  while\(bRedo\)\r\n  {\r\n      if\("ok" == global.ExtrCmmtPgsDlg.DoDialog\(\)\)\r\n      {\r\n          //Number of docs processed\r\n          global.nDocsProcessed = 0;\r\n\r\n          // Create Summary Doc\r\n      		global.oExtrSumDoc = app.newDoc\(649,459\);\r\n\r\n          // Add Fields to Doc and set data\r\n          CreateExtSumFields\(global.oExtrSumDoc,global.ExtrCmmtPgsDlg.strSummaryName,global.ExtrCmmtPgsDlg.bAppendToSum\);\r\n          \r\n\r\n          // Setup Path to extraction file\r\n          cSumPath = global.ExtrCmmtPgsDlg.strSummaryPath.replace\(/\([^/]\)$/, "$1/"\) \r\n                     + global.ExtrCmmtPgsDlg.strSummaryName.replace\(/\(\\s+\)/g,"_"\) + ".pdf";\r\n          try{\r\n             global.oExtrSumDoc.saveAs\(cSumPath\);\r\n             bRedo = false;\r\n          }catch\(e\){\r\n             var cRslt = app.alert\("Extraction Summary file could not be saved to:\\n" + cSumPath\r\n                              + "\\n\\nDo you want to reenter? \(Pressing No will"\r\n                              + " continue the Action with an unnamed Summary file\)"\r\n                              + " pressing Cancel will Stop the Action",1,3\);\r\n             switch\(cRslt\)\r\n             {\r\n                case 2: // Abort\r\n                  event.rc = false;\r\n                  bRedo = false;\r\n                  global.oExtrSumDoc.closeDoc\(true\);\r\n                  break;\r\n                case 3:  // Continue\r\n                  global.bSkipSumSave = true;\r\n                  bRedo = false;\r\n                  break;\r\n                case 4:  // Reenter\r\n                  bRedo = true;\r\n                  global.oExtrSumDoc.closeDoc\(true\);\r\n                  break;\r\n             }\r\n          }\r\n      }\r\n      else\r\n      { // Abort Action\r\n          event.rc = false;\r\n          bRedo = false;\r\n      }\r\n  }// Loop back for redo\r\n\r\n  console.println\("Setup Done"\);\r\n} \r\n\r\nif\(event.rc != false\)\r\n{\r\n  // Increment number of docs processed  \r\n  global.nDocsProcessed++;\r\n  global.oExtrSumDoc.getField\("NumFiles"\).value = "Files Processed: " + global.nDocsProcessed;\r\n  var cExtPath = "Not Set";\r\n  var cErrMsg = "";\r\n\r\n  var cItemSumText = "#" + global.nDocsProcessed + ": " + event.target.documentFileName + " - ";\r\n   console.println\("Working Doc: " + event.target.documentFileName\);\r\n  var oDoc = event.target;\r\n\r\n  if\(typeof\(event.target.xfa\)!="undefined"\)\r\n  {\r\n    cErrMsg = "XFA Form, Skipped";\r\n  }\r\n  else if\(global.oExtrSumDoc == event.target\)\r\n  {\r\n    cErrMsg = "Cannot Process Summary Doc, Skipped";\r\n  }\r\n  else if\(event.target.path[0] == "|"\)\r\n  {\r\n    cErrMsg = "Cannot Process Embedded Files, Skipped";\r\n  }\r\n  else\r\n  {// Only process non LiveCycle forms\r\n    // Data collection variable\r\n    var dataLine = "";\r\n\r\n    // Document must be saved for proper page Extraction.\r\n    if\(event.target.dirty\)\r\n    {\r\n      event.target.saveAs\(event.target.path\);\r\n    }\r\n		try {\r\n			// Wait until all comments have been scanned.\r\n			this.syncAnnotScan\(\);\r\n      var nPgsExtracted = 0;\r\n      var oExtDoc = null;\r\n\r\n      for\(var nPg=0;nPg<event.target.numPages;nPg++\)\r\n      {\r\n         //Look for Annots on each page\r\n         var oAnnots = event.target.getAnnots\(nPg\);\r\n         if\(oAnnots\)\r\n         {// found a page\r\n            nPgsExtracted++;\r\n\r\n            if\(global.ExtrCmmtPgsDlg.bSaveExtract\)\r\n            {// Save extracted pages to doc\r\n               // First check for existing extracted pages doc\r\n               if\(oExtDoc == null\)\r\n               {\r\n                 \r\n                 oExtDoc = event.target.extractPages\(nPg,nPg\);\r\n                 try{\r\n                   if\(global.ExtrCmmtPgsDlg.strSaveLoc=="Same"\)\r\n                     cExtPath = event.target.path.replace\(/\\/[^\\/]+$/,"/"\);\r\n                   else \r\n                     cExtPath = global.ExtrCmmtPgsDlg.strSavePath.replace\(/\([^/]\)$/, "$1/"\) ;\r\n                   cExtPath += event.target.documentFileName.replace\(/\\.pdf$/,"_CommentPages.pdf"\);\r\n\r\n                   console.println\("Save Extracted to: " + cExtPath\)\r\n                   oExtDoc.saveAs\(cExtPath\);\r\n                 }catch\(e\){\r\n                    app.alert\("Invalid Save Path:\\n"+cExtPath+ "\\n\\nAborting Action"\);\r\n                    event.rc = false;\r\n                 }\r\n                 if\(!event.rc\) break;\r\n               }\r\n               else\r\n                  oExtDoc.insertPages\(oExtDoc.numPages-1,event.target.path,nPg\);\r\n            }\r\n\r\n            if\(global.ExtrCmmtPgsDlg.bAppendToSum\)\r\n            {// Append extracted pages to Summary Doc\r\n                global.oExtrSumDoc.insertPages\(global.oExtrSumDoc.numPages-1,event.target.path,nPg\);\r\n                if\(nPgsExtracted == 1\)\r\n                {// First Page, Create Bookmark\r\n                   global.oExtrSumDoc.bookmarkRoot.createChild\(event.target.documentFileName,"this.pageNum=" + \(global.oExtrSumDoc.numPages-1\).toString\(\),\r\n                                                               global.oExtrSumDoc.bookmarkRoot.children.length\);\r\n                }\r\n            }\r\n\r\n         }\r\n      }\r\n      \r\n      if\(oExtDoc != null\)\r\n      {\r\n         oExtDoc.saveAs\(cExtPath\);\r\n         oExtDoc.closeDoc\(true\);\r\n      }\r\n\r\n    }catch\(e\){\r\n       app.alert\("Processing Error: Aborting Action\\n" + e\);\r\n       event.rc = false;\r\n    }\r\n  }\r\n\r\n  // Place File Name in Summary File;\r\n  var cItemSumText = event.target.documentFileName;\r\n  if\(event.rc\)\r\n  {\r\n     if\(cErrMsg.length\)\r\n        cItemSumText += " - " + cErrMsg;\r\n     else\r\n     {\r\n       cItemSumText += " - " + nPgsExtracted + " Pages Extracted";\r\n       if\(global.ExtrCmmtPgsDlg.bSaveExtract\)\r\n         cItemSumText += " to " + cExtPath;\r\n     }\r\n  }\r\n  else\r\n     cItemSumText += " - Processing Error, Action Aborted";\r\n\r\n  var oFld = global.oExtrSumDoc.getField\("FilesProcessed"\);\r\n  oFld.insertItemAt\(cItemSumText,oFld.numItems,-1\);\r\n  if\(!global.bSkipSumSave\)\r\n  {\r\n//     console.println\("Save Path: " + global.oExtrSumDoc.path\);\r\n     global.oExtrSumDoc.saveAs\(global.oExtrSumDoc.path\);\r\n  }\r\n  global.oExtrSumDoc.bringToFront\(\);\r\n}\r\n \r\n\r\n)]
